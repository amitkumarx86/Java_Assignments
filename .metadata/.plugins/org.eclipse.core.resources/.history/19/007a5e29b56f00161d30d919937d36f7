package sorting;

public class RadixSort {
    public static void main(String[] args) {
	int[] ar= {124,560,670,789,528,154,111};
	radixSort(ar);
    }
    public static void radixSort(int[] ar){
	int k=3;
	while(k-- >0)
	    for(int i=1;i<ar.length;i++){
		int j=i-1,key=((ar[i]/((int)Math.pow(10, i))))%10;
		
		
	    }
	}
    }
    
}
