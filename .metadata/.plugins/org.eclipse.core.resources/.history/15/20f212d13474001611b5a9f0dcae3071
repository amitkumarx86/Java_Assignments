    void bottom_to_Top(int[] ar,int i){
	while(i >=0 && ar[(i-1)/2] > ar[i]){
	    ar[(i-1)/2] = ar[i]+ar[(i-1)/2] - (ar[i]=ar[(i-1)/2]); // swapping
	}
    }
