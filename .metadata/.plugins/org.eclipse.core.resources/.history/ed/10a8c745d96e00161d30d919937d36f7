package Algorithms;

public class MaxProblems {
    public static void main(String[] args){
	int[] a = {1,2,3,4,5,-5,6,7,8};
	maxAbsSum(a);
    }
    public static void maxAbsSum(int[] a){
	int min=a[0],max=a[0];
	for(int i=1;i<a.length;i++){
	    if(min > a[i]) min = a[i];
	    if(max < a[i]) max = a[i];
	}
	System.out.println(Math.abs(max-min));
    }
}
