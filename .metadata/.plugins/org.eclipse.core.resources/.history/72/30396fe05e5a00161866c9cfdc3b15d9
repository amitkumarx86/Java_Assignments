package Algorithms;

import java.util.Scanner;

public class NonDivisibleSubset {
    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner read = new Scanner(System.in);
        int N = 6;//read.nextInt();
        int K = 9;//read.nextInt();
        long[] arr = {422346306, 940894801, 696810740, 862741861, 85835055, 313720373};//{1,2,4,7}; //new long[N];
        long[] bucket = new long[K];
        for(int i=0;i< K;i++)
            bucket[i]=0;
        for(int i=0; i< N ;i++){
            //arr[i] = read.nextLong();
            System.out.println("arr[i] = "+ arr[i] + "\t" + (int )arr[i]%K);
            bucket[(int)arr[i]%K] = bucket[(int)arr[i]%K] + 1;
        }
        
        long max = bucket[0];
        
        int index=0;
        int[] arr1 = new int[K];
        for(int i=0; i< K ;i++){
            arr1[i] = 0;
            if(max < bucket[i]){
        	index = i;
        	max = bucket[i];
            }
        }
        arr1[index] = -1;
        int count=0;        
        for(int i=0; i< K ;i++){
            if(max == bucket[i]){
        	arr1[i] = -1;
                count += max;
                bucket[i] = -1;
            }
        }
        System.out.println("--------------------");
        for(int i=0;i< K;i++){
            System.out.println("bucket[i] = " + bucket[i] + " arr1[i] ="+arr1[i]);
        }
        System.out.println("--------------------");
        for(int i=0;i< K;i++){
            System.out.println("--------------------");
            for(int j=0;j< K;j++){
        	if(arr1[j] == -1 && i+j % K != 0){
        	    count += bucket[i];
        	    arr1[i] = -1;
        	    //break;
        	    System.out.println("arr[j] = "+ arr1[j]  + " i = "+ i+ " j = " + j);
            	
        	}
            }
        }
        
        
        
        
        System.out.println(count);
        
        
        
        read.close();
    }
}
